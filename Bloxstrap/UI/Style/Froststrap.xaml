<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:base="clr-namespace:Bloxstrap.UI.Elements.Base">
    <SolidColorBrush x:Key="NewTextEditorBackground" Color="#59000000" />
    <SolidColorBrush x:Key="NewTextEditorForeground" Color="White" />
    <SolidColorBrush x:Key="NewTextEditorLink" Color="#3A9CEA" />
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#19000000" />
    <Color x:Key="ControlFillColorDefault">#19000000</Color>
    <LinearGradientBrush x:Key="WindowBackgroundGradient" StartPoint="1,1" EndPoint="0,0">
        <GradientStop Offset="0.0" Color="#6BA9B8" />
        <GradientStop Offset="0.4" Color="#4A9FB1" />
        <GradientStop Offset="0.75" Color="#346B7D" />
        <GradientStop Offset="1.0" Color="#275163" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="WindowTransparentBackgroundGradient" StartPoint="1,1" EndPoint="0,0">
        <GradientStop Offset="0.0" Color="#8C6BA9B8" />
        <GradientStop Offset="0.4" Color="#8C4A9FB1" />
        <GradientStop Offset="0.75" Color="#8C346B7D" />
        <GradientStop Offset="1.0" Color="#8C275163" />
    </LinearGradientBrush>
</ResourceDictionary>